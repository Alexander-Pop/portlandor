{#
/**
 * @file
 * Portland legacy paths custom block.
 */
#}
{% if legacy_paths %}
<div class="legacy-path-block">
   <h2>Legacy paths from portlandoregon.gov</h2>
   <div class="description">
      <p>{{ help_text }}</p>
   </div>
   <div class="legacy-paths">
      {% for path in legacy_paths %}
         <div class="legacy-path">
            <a href="{{ pog_base_url }}{{ path }}">{{ path }}</a>
         </div>
      {% endfor %}
   </div>
</div>
{% endif %}