      {
        url: 'example.com',
        text: 'breadcrumb 2'
      },
    ],
  } only %}
</div>

<div>
  {% include "@components/breadcrumb/breadcrumb.twig" with {
    items: [
      {
        text: 'breadcrumb'
      },
    ],
  } only %}
</div>

