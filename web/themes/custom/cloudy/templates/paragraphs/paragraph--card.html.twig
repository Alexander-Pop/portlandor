{% extends 'paragraph.html.twig' %}
{% block paragraph %}
  {% block field %}
    {% block content %}
      <div {{ attributes.addClass('card', 'clearfix', 'h-100') }}>
        <div class="card-body">
          <h4 class="card-title">{{ content.field_card_title }}</h4>
          {{ content.field_text }}
          {{ content.field_step_action_button}}
        </div>
      </div>
    {% endblock %}
  {% endblock %}
{% endblock %}