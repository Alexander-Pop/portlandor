{% embed "@cloudy/components/card.html.twig" with {
  'margin': 3,
  'type': node.type.entity.label,
  'description': content.field_summary,
} %}
  {% block meta %}
    {% include "@cloudy/components/meta.html.twig" with {
      'margin': 2,
      'start_date': content.field_start_date.0,
      'end_date': content.field_end_date.0,
      'all_day': content.field_all_day['#title'],
      'start_time': content.field_start_time.0,
      'end_time': content.field_end_time.0,
    } %}
  {% endblock %}
{% endembed %}
