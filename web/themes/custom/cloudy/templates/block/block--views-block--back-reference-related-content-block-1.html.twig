{% embed 'block.html.twig' with {
  'block_classes': [
    'mb-3',
    'alert',
    'alert-info',
    'border',
    'border-info',
  ]
} %}
{% block label %}
  {{ title_prefix }}
  {% if label %}
    <strong{{ title_attributes }}>{{ label }}</strong>
  {% endif %}
  {{ title_suffix }}
{% endblock %}
{% endembed %}
