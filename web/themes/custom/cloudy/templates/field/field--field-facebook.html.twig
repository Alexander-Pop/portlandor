{% embed 'field.html.twig' with {
  'margin' : '0'
  } %}
  {% spaceless %}
    {% block content %}
      {%- for item in items if item.content|render|striptags|trim is not empty -%}
        {% include "@components/links/facebook-link.twig" with {
          "username": item.content["#context"].value,
          "wrapper_class_markup": true,
          "wrapper_classes": [
            "flex"
          ]
        } only %}
      {%- endfor -%}
    {% endblock %}
  {% endspaceless %}
{% endembed %}
