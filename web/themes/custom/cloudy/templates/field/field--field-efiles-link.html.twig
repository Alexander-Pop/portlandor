{% set icon %}
  {% include '@elements/icons/archives.twig' with {
    "classes": [
      "h-5",
      "w-5"
    ]
  } %}
{% endset %}
{% embed 'field.html.twig' with {
  'margin' : '0'
  } %}
  {% spaceless %}
    {% block content %}
      {%- for item in items if item.content|render|striptags|trim is not empty -%}
        {% set text = item.content['#title'] == item.content['#url'].uri ? element['#object'].name.value : item.content['#title']%}
        {% include '@components/links/_icon-link.twig' with {
          'icon': icon,
          'text': text,
          'url': item.content['#url'].uri,
          "wrapper_class_markup": true,
          "wrapper_classes": [
            "flex"
          ]
        } only %}
      {%- endfor -%}
    {% endblock %}
  {% endspaceless %}
{% endembed %}
