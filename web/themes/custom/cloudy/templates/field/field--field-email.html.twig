{% embed 'field.html.twig' with {
  'margin' : '0'
  } %}
  {% spaceless %}
    {% block content %}
      {%- for item in items if item.content|render|striptags|trim is not empty -%}
        {% include "@components/links/email-link.twig" with {
          "email": item.content["#title"]
        } only %}
      {%- endfor -%}
    {% endblock %}
  {% endspaceless %}
{% endembed %}
